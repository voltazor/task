<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EmptyView">
        <attr name="android:text"/>
        <attr name="android:src"/>
        <attr name="srcCompat"/>
    </declare-styleable>

    <declare-styleable name="StyledInput">
        <attr name="android:text"/>
        <attr name="android:hint"/>
        <attr name="android:textSize"/>
        <attr name="android:textStyle"/>
        <attr name="android:textColor"/>
        <attr name="disabledTextColor" format="color"/>
        <attr name="hintTextSize" format="dimension"/>
        <attr name="hintTextColor" format="color"/>
        <attr name="hintTextStyle" format="reference"/>
        <attr name="errorEnabled" format="boolean"/>
        <attr name="errorText" format="string"/>
        <attr name="android:lines"/>
        <attr name="android:maxLines"/>
        <attr name="android:maxLength"/>
        <attr name="android:singleLine"/>
        <attr name="android:clickable"/>
        <attr name="hintAnimationEnabled" format="boolean"/>
        <attr name="android:inputType"/>
        <attr name="isMandatory" format="boolean"/>
        <attr name="removeUnderline" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RemoveButton">
        <attr name="android:text"/>
    </declare-styleable>

</resources>